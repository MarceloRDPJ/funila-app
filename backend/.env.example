# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# External Services
SOAWS_TOKEN=your_soawebservices_token
RESEND_API_KEY=re_your_resend_key

# Security (AES-256 Key for CPF Encryption)
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
ENCRYPTION_KEY=your_base64_encryption_key

# Environment
ENVIRONMENT=development
CORS_ORIGINS=http://localhost:3000,https://app.funila.com.br

# Master Admin Email (o email do admin do sistema)
# CRITICAL: Defina este valor no Render para evitar o email hardcoded
MASTER_EMAIL=seu-email@gmail.com

# Meta Ads OAuth (necessário para integração com Meta)
META_APP_ID=your_meta_app_id
META_APP_SECRET=your_meta_app_secret
META_REDIRECT_URI=https://api.funila.com.br/oauth/meta/callback

# Google Ads OAuth (opcional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://api.funila.com.br/oauth/google/callback

# Frontend URL (for OAuth redirects)
FRONTEND_URL=https://app.funila.com.br
